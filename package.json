{
  "name": "crm-react-scaffold-tool",
  "version": "0.0.1",
  "description": "React Scaffolding tool",
  "type": "module",
  "scripts": {
    "gen": "plop",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:watch": "vitest --coverage",
    "test:coverage:report": "vitest --coverage --reporter=html",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.hbs",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cashmy/crm-react-scaffold-tool.git"
  },
  "author": "Cash Myers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cashmy/crm-react-scaffold-tool/issues"
  },
  "homepage": "https://github.com/cashmy/crm-react-scaffold-tool#readme",
  "dependencies": {
    "axios": "^1.8.4",
    "chalk": "^5.4.1",
    "react": "^19.1.0",
    "react-router-dom": "^6.30.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.25.0",
    "@vitest/browser": "^3.1.1",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/ui": "^3.1.1",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-hbs": "^1.0.3",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "inquirer": "^12.5.2",
    "plop": "^4.0.1",
    "prettier": "^3.5.3",
    "vitest": "^3.1.1"
  }
}
